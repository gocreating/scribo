{
  "name": "AppUser",
  "plural": "app-users",
  "base": "User",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "realm": false,
    "credentials": false,
    "challenges": false,
    "emailVerified": false,
    "status": false,
    "created": false,
    "lastUpdated": false,
    "id": {
      "type": "string",
      "id": true
    },
    "username": {
      "type": "string",
      "required": true,
      "id": true
    },
    "email": {
      "type": "string",
      "required": true,
      "id": true
    },
    "createdAt": {
      "type": "date",
      "defaultFn": "now"
    }
  },
  "hidden": [
    "createdAt",
    "updatedAt"
  ],
  "mixins": {
    "TimeStamp": {
      "required": false
    }
  },
  "validations": [],
  "relations": {
    "posts": {
      "type": "hasMany",
      "model": "Post",
      "foreignKey": "authorId"
    }
  },
  "acls": [
    {
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW",
      "property": [
        "create",
        "xlogin",
        "logout",
        "findById",
        "__get__posts",
        "__findById__posts"
      ]
    },
    {
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW",
      "property": [
        "__create__posts",
        "__updateById__posts",
        "__destroyById__posts"
      ]
    }
  ],
  "methods": {}
}
