{
  "name": "Post",
  "plural": "posts",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "headerImage": {
      "type": "object"
    },
    "title": {
      "type": "string",
      "required": true
    },
    "subtitle": {
      "type": "string"
    },
    "slug": {
      "type": "string"
    },
    "abstractBlocks": {
      "type": [
        "object"
      ]
    },
    "blocks": {
      "type": [
        "object"
      ]
    },
    "createdAt": {
      "type": "date",
      "defaultFn": "now"
    }
  },
  "mixins": {
    "TimeStamp": {
      "required": false
    }
  },
  "validations": [],
  "relations": {
    "author": {
      "type": "belongsTo",
      "model": "AppUser",
      "foreignKey": "",
      "required": true
    }
  },
  "acls": [
    {
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW",
      "property": [
        "findMixed",
        "findById"
      ]
    }
  ],
  "methods": {}
}
